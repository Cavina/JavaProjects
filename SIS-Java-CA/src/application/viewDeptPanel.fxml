<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane prefHeight="447.0" prefWidth="608.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <TableView fx:id="deptTable" layoutY="55.0" prefHeight="327.0" prefWidth="680.0">
        <columns>
          <TableColumn fx:id="deptNameCol" prefWidth="75.0" text="Dept Name" />
          <TableColumn fx:id="deptHeadCol" prefWidth="75.0" text="Dept Head" />
          <TableColumn fx:id="idCol" prefWidth="115.0" text="ID " />
          <TableColumn fx:id="deptDescCol" prefWidth="414.0" text="Description" />
        </columns>
      </TableView>
      <HBox layoutX="14.0" layoutY="14.0" prefHeight="41.0" prefWidth="227.0">
         <children>
            <Label prefHeight="17.0" prefWidth="42.0" text="Filter:" />
            <TextField fx:id="filterField" />
         </children>
      </HBox>
      <ButtonBar layoutX="214.0" layoutY="14.0" prefHeight="41.0" prefWidth="65.0">
         <buttons>
            <Button mnemonicParsing="false" text="Reset" />
         </buttons>
      </ButtonBar>
      <ButtonBar layoutX="240.0" layoutY="382.0" prefHeight="40.0" prefWidth="200.0">
        <buttons>
          <Button fx:id="editButton" mnemonicParsing="false" text="Edit Selected" />
            <Button fx:id="deleteButton" mnemonicParsing="false" prefHeight="25.0" prefWidth="126.0" text="Delete Selected" />
        </buttons>
      </ButtonBar>
   </children>
</AnchorPane>
