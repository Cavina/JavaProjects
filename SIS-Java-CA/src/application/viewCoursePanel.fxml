      <TableView fx:id="courseTable" layoutY="55.0" prefHeight="327.0" prefWidth="680.0">
        <columns>
          <TableColumn fx:id="courseNameCol" prefWidth="87.0" text="Course Name" />
          <TableColumn fx:id="courseNumCol" prefWidth="93.0" text="Course Number" />
            <TableColumn fx:id="semesterCol" prefWidth="60.0" text="Semester" />
            <TableColumn fx:id="yearCol" prefWidth="60.0" text="Year" />
            <TableColumn fx:id="instructorCol" prefWidth="85.0" text="Instructor" />
            <TableColumn fx:id="deptCol" prefWidth="73.0" text="Dept" />
            <TableColumn fx:id="sIDCol" prefWidth="64.0" text="Section ID" />
            <TableColumn fx:id="descriptionCol" prefWidth="154.0" text="Description" />
            
        </columns>
      </TableView>
      <HBox layoutX="14.0" layoutY="14.0" prefHeight="41.0" prefWidth="227.0">
         <children>
            <Label prefHeight="17.0" prefWidth="42.0" text="Filter:" />
