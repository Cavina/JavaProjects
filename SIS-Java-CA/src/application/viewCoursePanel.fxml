      </ButtonBar>
      <ButtonBar layoutX="205.0" layoutY="382.0" prefHeight="40.0" prefWidth="200.0">
        <buttons>
          <Button mnemonicParsing="false" text="Edit Selected" />
            <Button fx:id="deleteButton" mnemonicParsing="false" prefHeight="25.0" prefWidth="126.0" text="Delete Selected" />
        </buttons>
      </ButtonBar>
   </children>
</AnchorPane>
