          <TableColumn fx:id="genderCol" prefWidth="75.0" text="Gender" />
          <TableColumn fx:id="addressCol" prefWidth="75.0" text="Address" />
          <TableColumn fx:id="zipCol" prefWidth="75.0" text="Zip" />
          <TableColumn fx:id="phoneCol" prefWidth="75.0" text="Phone" />
          <TableColumn fx:id="bdayCol" prefWidth="75.0" text="Birthday" />
          <TableColumn fx:id="emailCol" prefWidth="75.0" text="E-mail" />
          <TableColumn fx:id="IDCol" prefWidth="99.0" text="ID " />
        </columns>
      </TableView>
      <HBox layoutX="14.0" layoutY="14.0" prefHeight="41.0" prefWidth="227.0">
